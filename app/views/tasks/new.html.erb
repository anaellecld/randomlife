<h1><%= @category.title %></h1>


<div class="container">
  <div class="row">
    <div class="container formnew">
      <%= simple_form_for [@category, @task] do |f| %>
        <%= f.input :name, label: "Who is doing..? (put your task)" %>
        <%= f.input :start_date %>
        <br>
        <%= f.button :submit, 'Go', class:'btn btn-ghost' %>
      <% end %>
    </div>
  </div>
</div>




