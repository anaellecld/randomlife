<h1>Categories#new</h1>
<p>Find me in app/views/categories/new.html.erb</p>

<div class="container formnew">
  <%= simple_form_for @category do |f| %>
  <%= f.input :title %>
  <%= f.input :kind, priority: [ "Task" ], collection: [ "Task", "Event"] %>
  <br>
  <%= f.button :submit, 'Create' , class:'btn btn-ghost' %>
  <% end %>
</div>


